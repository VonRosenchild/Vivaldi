fn filter(arr, pred): do
  let new_arr = []
  for i in arr: if pred(i): new_arr.append(i)
  new_arr
end

fn primes(upto): do
  let candidates = []
  for i in 2 to upto: candidates.append(i)
  let found = []
  while candidates.at(0) < upto ** 0.5: do
    let prime = candidates.at(0)
    found.append(prime)
    candidates = filter(candidates, fn (x): x % prime != 0)
  end
  found + candidates
end

let p = primes(10000)
puts(p)
puts(p.size())
